@article{doi:10.1142/S0217751X04019676,
author = {ZHU, SHI-LIN},
title = {PENTAQUARKS},
journal = {International Journal of Modern Physics A},
volume = {19},
number = {21},
pages = {3439-3469},
year = {2004},
doi = {10.1142/S0217751X04019676},

URL = { 
        https://doi.org/10.1142/S0217751X04019676
    
},
eprint = { 
        https://doi.org/10.1142/S0217751X04019676
    
}
,
    abstract = { Since LEPS collaboration reported the first evidence of Θ+ pentaquark in early 2003, eleven other experimental groups have confirmed this exotic state while many other groups did not see any signal. If this state is further established by future high statistical experiments, its discovery shall be one of the most important events in hadron physics for the past three decades. This exotic baryon with such a low mass and so narrow a width imposes a big challenge to hadron theorists. Up to now, there have appeared more than two hundred theoretical papers trying to interpret this charming state. I will review some important theoretical developments on pentaquarks based on my biased personal views. }
}



