author = {Liu, Tianbo and Mao, Yajun and Ma, Bo-Qiang},
title = {Present status on experimental search for pentaquarks},
journal = {International Journal of Modern Physics A},
volume = {29},
number = {13},
pages = {1430020},
year = {2014},
doi = {10.1142/S0217751X14300208},

URL = { 
        https://doi.org/10.1142/S0217751X14300208
    
},
eprint = { 
        https://doi.org/10.1142/S0217751X14300208
    
}
,
    abstract = { It has been ten years since the first report for a positive strangeness pentaquark-like baryon state. However, the existence of the pentaquark state is still controversial. Some contradictions between the experiments are unsolved. In this paper, we review the experimental search for the pentaquark candidates Θ+, Θ++, Ξ- -, \$\Theta\_c^0\$ and N* in detail. We review the experiments with positive results and compare the experiments with similar conditions but opposite results. }
}


